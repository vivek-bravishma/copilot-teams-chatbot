{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "ViveK Nishad",
        "websiteUrl": "https://www.bravishma.com/",
        "privacyUrl": "https://www.bravishma.com/privacy-policy.html",
        "termsOfUseUrl": "https://www.bravishma.com/termofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "IT Support${{APP_NAME_SUFFIX}}",
        "full": "IT Support"
    },
    "description": {
        "short": "Avaya IT Support",
        "full": "Bot for Avaya IT Support"
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}